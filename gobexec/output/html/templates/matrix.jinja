{% set result = this %}
<table>
<tr>
    <th>Group</th>
    <th>Benchmark</th>
{% for tool in result.tools %}
    <th>{{ tool.name }}</th>
{% endfor %}
</tr>
{% for group_result in result.groups %}
    {% for benchmark_results in group_result.benchmarks %}
        <tr>
        {% if loop.first %}
            <td rowspan={{ group_result.benchmarks|length }} style="vertical-align: top;">{{ group_result.group.name }}</td>
        {% endif %}
            <td>{{ benchmark_results.benchmark.name }}</td>
        {% for benchmark_result in benchmark_results.results %}
            <td>
            {% if benchmark_result.result %}
            {{ benchmark_result.result|template }}
            {% endif %}
            </td>
        {% endfor %}
        </tr>
    {% endfor %}
{% endfor %}
</table>
